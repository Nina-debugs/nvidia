                            email: email,
                            country: country
                        });
                    }
                    app.trigger('form-submit', payload);

                    // Redirect to Configured Thank You URL
                    var thankYouUrl = me.widget() && me.widget().settings().thankYouUrl;
                    if (thankYouUrl) {
                        setTimeout(function () {
                            window.top.location = thankYouUrl;
                        }, 5000);
                    }
                }
                else {
